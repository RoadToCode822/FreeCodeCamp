function largestOfFour(arr) {
  var num = [];
  var longest = 0;
  for (i=0; i<arr.length; i++){
    var subArray = arr[i];
      for(j=0; j<subArray.length; j++){
        if(longest < subArray[j]) {
        longest = subArray[j];
    }
        num.push(longest);
    
  }
  return num;
}

largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
